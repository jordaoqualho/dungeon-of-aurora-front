{
  "name": "dungeonsofaurora",
  "private": true,
  "version": "0.6.4",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "start": "vite",
    "start:dev": "set VITE_ENVIRONMENT=development&& vite --host",
    "start:prod": "set VITE_ENVIRONMENT=production&& vite --host",
    "prepare": "husky install",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint && build"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@react-oauth/google": "^0.11.1",
    "@tanstack/react-query": "^5.51.23",
    "@types/lodash": "^4.17.7",
    "@vercel/speed-insights": "^1.0.12",
    "axios": "^1.7.4",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-swipe-to-delete-ios": "^2.1.0",
    "react-toastify": "^9.1.3",
    "release-it": "^17.6.0",
    "styled-components": "^6.1.12",
    "usehooks-ts": "^2.16.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "standard-version": "^9.5.0",
    "typescript": "5.1.6",
    "vite": "^4.5.3",
    "vite-plugin-pwa": "^0.16.7",
    "workbox-window": "^7.1.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: ðŸ¤– release v${version}"
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
