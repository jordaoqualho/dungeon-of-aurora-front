{
  "name": "dungeonsofaurora",
  "private": true,
  "version": "0.1.15",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "start:dev": "set VITE_ENVIRONMENT=development&& vite --host",
    "start:prod": "set VITE_ENVIRONMENT=production&& vite",
    "prepare": "husky install",
    "push": "npm version patch && git push",
    "push-minor": "npm version minor && git push",
    "push-major": "npm version major && git push",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@react-oauth/google": "^0.11.1",
    "@tanstack/react-query": "^5.8.7",
    "axios": "^1.6.2",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.1.1",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.32",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.7",
    "workbox-window": "^7.0.0"
  }
}
